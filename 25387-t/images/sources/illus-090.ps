/GoDown { 1 FontScale -0.15 mul rm } def
/GonwoD { 1 FontScale 0.15 mul rm } def
/GoUp { 1 FontScale 0.42 mul rm } def
/GopU { 1 FontScale -0.42 mul rm } def
/Super { currentfont .7 scalefont setfont GoUp
 /FontScale FontScale .7 mul def exec show
 /FontScale FontScale .7 div def GopU 
 currentfont 1 .7 div scalefont setfont () } def
/Sub { currentfont .7 scalefont setfont GoDown
 /FontScale FontScale .7 mul def exec show
 /FontScale FontScale .7 div def GonwoD
 currentfont 1 .7 div scalefont setfont () } def

